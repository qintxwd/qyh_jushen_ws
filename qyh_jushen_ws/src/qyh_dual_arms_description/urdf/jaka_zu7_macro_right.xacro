<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">
  
  <!-- JAKA Zu7 单臂宏定义 - 完全匹配Dual-Arms参考配置 -->
  <xacro:macro name="jaka_zu7_arm_right" params="parent *origin">
    
    <!-- 根据左右臂设置不同的关节旋转参数 -->
    <xacro:property name="j2_rpy" value="-1.5708 0 -3.1416"/>
    <xacro:property name="j6_rpy" value="-1.5708 0 -3.1416"/>

    <!-- 基座连接 -->
    <joint name="right_base_joint" type="fixed">
      <xacro:insert_block name="origin"/>
      <parent link="${parent}"/>
      <child link="right_base_link"/>
    </joint>

    <link name="right_base_link">
      <visual>
        <origin xyz="-0.01 0.21 0.21" rpy="1.5708 0 0"/>
        <geometry>
          <mesh filename="package://qyh_dual_arms_description/meshes/right.stl"/>
        </geometry>
        <material name="LightGrey">
            <color rgba="0.7 0.7 0.7 1.0"/>
        </material>
      </visual>
      <collision>
        <origin xyz="-0.01 0.21 0.21" rpy="1.5708 0 0"/>
        <geometry>
          <mesh filename="package://qyh_dual_arms_description/meshes/right.stl"/>
        </geometry>
      </collision>
      <inertial>
         <origin xyz="0 0 0.05" rpy="0 0 0"/>
         <mass value="2.0"/>
         <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
      </inertial>
    </link>

    <!-- Joint 1: 从底座顶部向上，添加第一个关节 -->
    <joint name="right_joint1" type="revolute">
      <origin xyz="0 0 0.217" rpy="0 0 0"/>
      <parent link="right_base_link"/>
      <child link="right_link1"/>
      <axis xyz="0 0 1"/>
      <limit lower="-6.2832" upper="6.2832" effort="0" velocity="0"/>
    </joint>

    <link name="right_link1">
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://qyh_dual_arms_description/meshes/r1.STL"/>
        </geometry>
        <material name="LightGrey">
            <color rgba="0.7 0.7 0.7 1.0"/>
        </material>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://qyh_dual_arms_description/meshes/r1.STL"/>
        </geometry>
      </collision>
      <inertial>
         <origin xyz="0 0 0.16" rpy="0 0 0"/>
         <mass value="1.5"/>
         <inertia ixx="0.003" ixy="0" ixz="0" iyy="0.003" iyz="0" izz="0.002"/>
      </inertial>
    </link>

    <!-- Joint 2 -->
    <joint name="right_joint2" type="revolute">
      <origin xyz="0 0 0.2075" rpy="${j2_rpy}"/>
      <parent link="right_link1"/>
      <child link="right_link2"/>
      <axis xyz="0 0 1"/>
      <limit lower="-1.8325" upper="1.8325" effort="0" velocity="0"/>
    </joint>
    
    <link name="right_link2">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://qyh_dual_arms_description/meshes/r2.STL"/>
            </geometry>
            <material name="LightGrey"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://qyh_dual_arms_description/meshes/r2.STL"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 -0.045 0" rpy="0 0 0"/>
            <mass value="2.7"/>
            <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.006" iyz="0" izz="0.007"/>
        </inertial>
    </link>

    <!-- Joint 3 -->
    <joint name="right_joint3" type="revolute">
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <parent link="right_link2"/>
      <child link="right_link3"/>
      <axis xyz="0 0 1"/>
      <limit lower="-6.2832" upper="6.2832" effort="0" velocity="0"/>
    </joint>

    <link name="right_link3">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://qyh_dual_arms_description/meshes/r3.STL"/>
            </geometry>
            <material name="LightGrey"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://qyh_dual_arms_description/meshes/r3.STL"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0.23" rpy="0 0 0"/>
            <mass value="3.5"/>
            <inertia ixx="0.018" ixy="0" ixz="0" iyy="0.018" iyz="0" izz="0.003"/>
        </inertial>
    </link>

    <!-- Joint 4 -->
    <joint name="right_joint4" type="revolute">
      <origin xyz="-0.00040027 0 0.33028" rpy="-1.5708 0 0"/>
      <parent link="right_link3"/>
      <child link="right_link4"/>
      <axis xyz="0 0 1"/>
      <limit lower="-2.5307" upper="0.5235" effort="0" velocity="0"/>
    </joint>

    <link name="right_link4">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://qyh_dual_arms_description/meshes/r4.STL"/>
            </geometry>
            <material name="LightGrey"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://qyh_dual_arms_description/meshes/r4.STL"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 -0.03 0" rpy="0 0 0"/>
            <mass value="1.5"/>
            <inertia ixx="0.003" ixy="0" ixz="0" iyy="0.002" iyz="0" izz="0.002"/>
        </inertial>
    </link>

    <!-- Joint 5 -->
    <joint name="right_joint5" type="revolute">
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <parent link="right_link4"/>
      <child link="right_link5"/>
      <axis xyz="0 0 1"/>
      <limit lower="-6.2832" upper="6.2832" effort="0" velocity="0"/>
    </joint>

    <link name="right_link5">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://qyh_dual_arms_description/meshes/r5.STL"/>
            </geometry>
            <material name="LightGrey"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://qyh_dual_arms_description/meshes/r5.STL"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0.18" rpy="0 0 0"/>
            <mass value="2.3"/>
            <inertia ixx="0.005" ixy="0" ixz="0" iyy="0.005" iyz="0" izz="0.002"/>
        </inertial>
    </link>

    <!-- Joint 6 -->
    <joint name="right_joint6" type="revolute">
      <origin xyz="0 0 0.23494" rpy="${j6_rpy}"/>
      <parent link="right_link5"/>
      <child link="right_link6"/>
      <axis xyz="0 0 1"/>
      <limit lower="-1.8325" upper="1.8325" effort="0" velocity="0"/>
    </joint>

    <link name="right_link6">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://qyh_dual_arms_description/meshes/r6.STL"/>
            </geometry>
            <material name="LightGrey"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://qyh_dual_arms_description/meshes/r6.STL"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 -0.08 0" rpy="0 0 0"/>
            <mass value="1.7"/>
            <inertia ixx="0.004" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.004"/>
        </inertial>
    </link>

    <!-- Joint 7 -->
    <joint name="right_joint7" type="revolute">
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <parent link="right_link6"/>
      <child link="right_link7"/>
      <axis xyz="0 0 1"/>
      <limit lower="-6.2832" upper="6.2832" effort="0" velocity="0"/>
    </joint>

    <link name="right_link7">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://qyh_dual_arms_description/meshes/r7.STL"/>
            </geometry>
            <material name="LightGrey"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://qyh_dual_arms_description/meshes/r7.STL"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0.17" rpy="0 0 0"/>
            <mass value="0.6"/>
            <inertia ixx="0.0003" ixy="0" ixz="0" iyy="0.0003" iyz="0" izz="0.0005"/>
        </inertial>
    </link>

    <!-- Tool Joint -->
    <joint name="right_tool_joint" type="fixed">
      <origin xyz="0 0 0.20277" rpy="0 0 0"/>
      <parent link="right_link7"/>
      <child link="right_tool0"/>
    </joint>

    <link name="right_tool0">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://qyh_dual_arms_description/meshes/rt.STL"/>
            </geometry>
            <material name="LightGrey"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://qyh_dual_arms_description/meshes/rt.STL"/>
            </geometry>
        </collision>
    </link>

  </xacro:macro>

</robot>
